{
  "name": "convue",
  "version": "1.1.5",
  "description": "convue is a plugin for vite based on vite and vue3 that gives you a set of experiences for rapid development projects, similar to nuxt and umi.js.",
  "author": "ziping <575936902@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ziping-li/convue.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "convue",
    "Convue",
    "vite",
    "Vue",
    "vue3",
    "plugin",
    "framework",
    "cli",
    "umi",
    "nuxt"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "tsx:dev": "npm -C examples/tsx run dev",
    "tsx:build": "npm -C examples/tsx run build",
    "tsx:serve": "npm -C examples/tsx run serve",
    "sfc:dev": "npm -C examples/tsx run dev",
    "sfc:build": "npm -C examples/tsx run build",
    "sfc:serve": "npm -C examples/tsx run serve",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{vue,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "ant-design-vue": "^2.0.0-rc.9",
    "axios": "^0.21.1",
    "js-cookie": "^2.2.1",
    "nprogress": "^0.2.0",
    "vite": "^2.0.0-beta.64",
    "vue": "^3.0.5",
    "vue-i18n": "^9.0.0-rc.6",
    "vue-router": "4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@types/debug": "^4.1.5",
    "@types/deep-equal": "^1.0.1",
    "@types/glob-to-regexp": "^0.4.0",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.14.28",
    "@vitejs/plugin-vue": "^1.1.2",
    "@vitejs/plugin-vue-jsx": "^1.0.2",
    "@vue/babel-plugin-jsx": "^1.0.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "ant-design-vue": "^2.0.0-rc.9",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "css-loader": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "file-loader": "^6.0.0",
    "husky": "^4.3.8",
    "js-cookie": "^2.2.1",
    "less": "^4.1.0",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.4",
    "nprogress": "^0.2.0",
    "prettier": "^2.2.1",
    "rollup": "^2.38.5",
    "standard-version": "^9.1.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-loader": "^8.0.4",
    "tsup": "^3.12.1",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.0",
    "vite": "^2.0.0-beta.50",
    "vitepress": "^0.12.2",
    "vue": "^3.0.5",
    "vue-i18n": "^9.0.0-rc.6",
    "vue-loader": "^16.0.0-beta.7",
    "vue-router": "4",
    "vuex": "^4.0.0"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.0.5",
    "chalk": "^4.1.0",
    "debug": "^4.3.2",
    "deep-equal": "^2.0.5",
    "directory-tree": "^2.2.6",
    "fast-glob": "^3.2.5",
    "glob-to-regexp": "^0.4.1",
    "json5": "^2.2.0",
    "yaml": "^2.0.0-3"
  }
}
